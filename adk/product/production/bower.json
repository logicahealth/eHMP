  "install": {
    "base": "_assets/libs/bower",
    "path": {
      "/backbone[-|.](?!js|min)/": "backbone/{key}",
      "/backgrid[-]/": "backgrid/{key}",
      "/bootstrap[-](?!theme)/": "bootstrap/{key}",
      "/(bootstrap|font-awesome).*(css)$/": "{key}/css",
      "/(handlebars|json2|underscore).(js)$/": "{key}/{key}",
      "js": "{key}",
      "rb": "{key}",
      "map": "{key}",
      "/[c|sc|le]ss$/": "{key}",
      "/(eot|svg|ttf|woff|woff2|otf])$/": "{key}/fonts",
      "/(gif|png)$/": "{key}"
    },
    "sources": {
      "async": "bower_components/async/lib/async.js",
      "backbone": "bower_components/backbone/backbone.js",
      "backbone-sessionStorage": "bower_components/backbone-sessionStorage/backbone.sessionStorage.js",
