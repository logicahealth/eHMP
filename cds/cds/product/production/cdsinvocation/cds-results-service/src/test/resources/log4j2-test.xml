<?xml version="1.0" encoding="UTF-8"?>
<configuration status="debug">
        <Appenders>
                <Console name="Console" target="SYSTEM_OUT">
                        <PatternLayout pattern="%d{HH:mm:ss} [%t] %-5level %logger{36} - %msg%n" />
                </Console>
        </Appenders>
        <Loggers>
                <Root level="debug">
                        <AppenderRef ref="Console" />
                </Root>
        </Loggers>
</configuration>
