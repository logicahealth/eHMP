grails.jmeadowsURL = <%= jmeadows[:jmeadows][:application][:protocol]%>://<%= jmeadows[:jmeadows][:ipaddress]%><%= jmeadows[:jmeadows][:application][:path]%>?<%= jmeadows[:jmeadows][:application][:path_query]%>
#http://IP          /jMeadows/JMeadowsDataService?wsdl

grails.jmeadowsUsername = REDACTED
grails.jmeadowsPassword = REDACTED

grails.smartCardBypass = true
grails.testSmartCard = 987654322
grails.isEncryptedTokenDebug = false
grails.trustAllSSLHostnamesAndCerts = false

connectionTimeoutMS = 45000
requestTimeoutMS = 100000

grails.extLib = ext-3.4.0

#grails tomcat plugin 2 way SSL connector configurations
grails.enableTomcatPlugin2WaySSL = false
grails.tomcatPlugin2WaySSLKeystoreFile = C:\\glassfish\\domains\\domain1\\config\\keystore.jks
grails.tomcatPlugin2WaySSLKeystorePass = changeit
grails.tomcatPlugin2WaySSLTruststoreFile = C:\\glassfish\\domains\\domain1\\config\\jssecacerts
grails.tomcatPlugin2WaySSLTruststorePass = changeit
